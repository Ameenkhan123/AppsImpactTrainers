<div class="message">
  <a href="" class="message_profile-pic"></a>
  <a href="" class="message_username"><%= content_with_emoji(message.user.try(:name)) %></a>
  <span class="message_timestamp">
    <%= message.created_at %>
  </span>
  <span class="message_star"></span>
  <span class="message_content">
    <%= message.content %>
  </span>
</div>
